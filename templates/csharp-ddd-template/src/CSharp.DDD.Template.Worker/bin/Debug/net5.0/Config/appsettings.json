{
  "CenterId": 0,
  "WorkId": 0,
  "Quartz": {
    "Open": true,
    //触发器
    "CronTriggers": [
      {
        "Open": true, //功能开关
        //基于镜像回源文件同步任务
        "Name": "ExampleCronJob_Trigger",
        //分组
        "Group": "ExampleCronJob_Triggers",
        //表达式（5秒钟执行一次）
        "Expression": "*/1 * * * * ?",
        "JobType": "CSharp.DDD.Template.Application.Jobs.ExampleCronJob,CSharp.DDD.Template.Application",
        "JobName": "ExampleCronJob",
        "JobGroup": "ExampleCronJobGroup"
      }
    ]
  }
}